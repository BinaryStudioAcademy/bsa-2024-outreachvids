        "postcss": "8.4.41",
        "vite": "5.4.0"
    },
    "dependencies": {
        "@chakra-ui/react": "2.8.2",
        "@emotion/react": "11.13.0",
        "@emotion/styled": "11.13.0",
        "@reduxjs/toolkit": "2.2.7",
        "@remotion/player": "4.0.201",
        "formik": "2.4.6",
        "framer-motion": "11.3.24",
        "react": "18.3.1",
        "react-dom": "18.3.1",
